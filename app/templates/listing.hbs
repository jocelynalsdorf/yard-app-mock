<div class="container listing-solo">
	<div class="row">

		<div class="col-lg-5">
			<div class="panel panel-default text-center">
          <div class="panel-body listing-info-panel">
            <span class="glyphicon glyphicon-ok"></span>
            <h4>This is heading section 1</h4>
            <p>This is text for section 1</p>
          </div><!-- end of panel-body div -->
        </div><!-- end of panel-default div -->
		</div><!-- end of col-5 -->

		<div class="col-lg-7">

			<div class="panel panel-default text-center">
        <div class="panel-body listing-map-panel">
          <span class="glyphicon glyphicon-ok"></span>
          <h4>This is heading section 1</h4>
          <p>This is text for section 1</p>
        </div><!-- end of panel-body div -->
      </div><!-- end of panel-default div -->
<!--carousel slide section -->

        <h4 class="image-header">{{listing.name}} Images</h4>

      <div class="carousel slide caro-solo" id="screenshot-carousel" data-ride="carousel">

        <ol class="carousel-indicators">
          <li data-target="#screenshot-carousel" data-slide-to="0" class="active"></li>
          <li data-target="#screenshot-carousel" data-slide-to="1"></li>
          <li data-target="#screenshot-carousel" data-slide-to="2"></li>
          <li data-target="#screenshot-carousel" data-slide-to="3"></li>
        </ol>

        <div class="carousel-inner">

          <div class="item active">
            <img class="big-img" src="../cat3.jpg" alt="pic 1"/>
            <div class="carousel-caption">
              <h3>Big heading</h3>
              <p>Smaller subheading stuff</P>
            </div>
          </div>
          <div class="item">
            <img class="big-img" src="../cat3.jpg" alt="pic 2"/>
            <div class="carousel-caption">
              <h3>Big heading</h3>
              <p>Smaller subheading stuff</P>
            </div>
          </div>
          <div class="item">
            <img class="big-img" src="../cat3.jpg" alt="pic 3"/>
            <div class="carousel-caption">
              <h3>Big heading</h3>
              <p>Smaller subheading stuff</P>
            </div>
          </div>
          <div class="item">
            <img class="big-img" src="../cat4.jpg" alt="pic 4"/>
              <div class="carousel-caption">
                <h4>{{listing.name}}</h4>
              </div>
          </div>

        </div><!-- end of inner carousel -->

        <!-- add side arrows to carousel -->
        <a href="#screenshot-carousel" class="left carousel-control" data-slide="prev">
          <span class="glyphicon glyphicon-chevron-left"></span>
        </a>
        <a href="#screenshot-carousel" class="right carousel-control" data-slide="next">
          <span class="glyphicon glyphicon-chevron-right"></span>
        </a>
      </div><!-- end of carousel slide div -->	

		</div><!-- end of col-7 -->
	</div><!--end of row -->
</div><!-- end of solo listing container -->